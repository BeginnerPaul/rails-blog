<h1>Articles</h1>

<ul>
    <% @articles.each do |article| %>
        <% unless article.archived? %>
            <li>
                <%= link_to article.title, article %>
            </li>
            <%#= article.created_at %>  
        <% end %>
    <% end %>   
</ul>

<%= link_to "Create new article", new_article_path %>
